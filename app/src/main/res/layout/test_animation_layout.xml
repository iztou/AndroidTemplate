<?xml version="1.0" encoding="utf-8"?>
<gml.template.androidtemplate.SwipeAndOtherLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/first"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:text="测试滑动并缩小"
        android:textSize="16sp"
        android:gravity="center"
        android:background="#ff00ddff"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_below="@+id/first"
        android:background="#ff11ddaa"/>
</gml.template.androidtemplate.SwipeAndOtherLayout>
